<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/tintedBlue">

    <LinearLayout
        android:id="@+id/header_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">
        <LinearLayout
            android:id="@+id/gameNight_header"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Spieleabend der Gruppe:"
                android:layout_marginTop="10sp"
                android:gravity="start"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:paddingStart="10sp"/>
            <TextView
                android:id="@+id/gameNight_header_groupname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:gravity="start"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:paddingStart="10sp"
                android:paddingBottom="10sp"
                android:textStyle="bold"/>
        </LinearLayout>
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            app:cardCornerRadius="10dp"
            app:cardElevation="8dp"
            app:cardUseCompatPadding="true">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/gameNight_ratings_headline"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:gravity="center"
                    android:paddingTop="8sp"
                    android:paddingBottom="10sp"
                    android:text="Bewertungen"
                    android:textColor="@color/lightGold"
                    android:textSize="20sp"
                    android:textStyle="bold" />
                <TableRow>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:background="@color/tintedBlue"
                        android:text=""
                        android:layout_weight="1"
                        android:textColor="@color/lightGold"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text="Gastgeber"
                        android:layout_weight="1"
                        android:background="@color/tintedBlue"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text="Essen"
                        android:layout_weight="1"
                        android:background="@color/tintedBlue"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text="Spieleabend"
                        android:layout_weight="1"
                        android:background="@color/tintedBlue"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                </TableRow>
                <TableRow>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:paddingStart="8sp"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text="Gesamt"
                        android:layout_weight="1"
                        android:background="@color/tintedBlue"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_host_total"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/lightGold"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_food_total"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/lightGold"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_total_total"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/lightGold"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                </TableRow>
                <TableRow>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:paddingTop="8sp"
                        android:paddingStart="8sp"
                        android:paddingBottom="10sp"
                        android:text="Meine Bewertung"
                        android:layout_weight="1"
                        android:background="@color/tintedBlue"
                        android:textColor="@color/black"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_host_myRating"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/green"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_food_myRating"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/green"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/gameNight_ratings_total_myRating"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingTop="8sp"
                        android:paddingBottom="10sp"
                        android:text=""
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:textColor="@color/green"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                </TableRow>
            </TableLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header_layout"
        app:layout_constraintBottom_toBottomOf="parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:background="@color/blue"
                android:gravity="center"
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:text="Wann / Wo"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/header_layout" />
            <androidx.cardview.widget.CardView
                android:id="@+id/gameNight_timePlace_cardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="8dp"
                app:cardUseCompatPadding="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/gameNight_dateTime_value"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="01.01.2024\n11:11 Uhr"
                            android:textSize="15sp"
                            android:layout_gravity="center"
                            android:paddingStart="10sp"
                            android:paddingTop="10sp"
                            android:paddingBottom="10sp"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:textStyle="bold"/>
                        <Button
                            android:id="@+id/gameNight_timePlace_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginBottom="10sp"
                            android:layout_weight="2"
                            android:background="@drawable/buttton"
                            android:gravity="center"
                            android:text="Ändern"
                            android:textColor="@color/white"
                            android:textSize="13sp"/>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/gameNight_hostName_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Gastgeber:"
                        android:textSize="15sp"
                        android:layout_gravity="center"
                        android:paddingStart="10sp"
                        android:paddingTop="10sp"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textStyle="bold"
                        android:textColor="@color/lightGold"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/gameNight_hostName_value"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Peter Griffon"
                            android:textSize="15sp"
                            android:layout_gravity="center"
                            android:paddingStart="10sp"
                            android:paddingTop="10sp"
                            android:paddingBottom="10sp"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:textStyle="bold"/>
                        <Button
                            android:id="@+id/gameNight_changeHost_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10sp"
                            android:layout_weight="2"
                            android:background="@drawable/buttton"
                            android:gravity="center"
                            android:text="Ändern"
                            android:textColor="@color/white"
                            android:textSize="13sp"/>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/gameNight_hostStreetHousnumber_value"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Peterstraße 1"
                        android:textSize="15sp"
                        android:layout_gravity="center"
                        android:paddingStart="10sp"
                        android:paddingBottom="5sp"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/gameNight_hostTownPostalcode_value"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:paddingStart="10sp"
                        android:paddingBottom="10sp"
                        android:text="23456 Petercity"
                        android:textSize="15sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:background="@color/blue"
                android:gravity="center"
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:text="Abstimmung"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/header_layout" />
            <androidx.cardview.widget.CardView
                android:id="@+id/gameNight_gameVoting_cardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="8dp"
                app:cardUseCompatPadding="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/gameNight_proposalHeader_game"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/tintedBlue"
                        android:text="Spiele"
                        android:textSize="15sp"
                        android:paddingTop="10sp"
                        android:paddingBottom="10sp"
                        android:textColor="@color/lightGold"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/gameNight_gameProposal_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Spiel vorschlagen:"
                        android:textSize="15sp"
                        android:layout_gravity="center"
                        android:paddingStart="10sp"
                        android:paddingTop="10sp"
                        android:textColor="@color/lightGold"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="10sp">
                        <EditText
                            android:id="@+id/gameNight_gameProposal_value"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="15sp"
                            android:layout_gravity="center"
                            android:background="@drawable/boarder"
                            android:paddingStart="10sp"
                            android:paddingTop="10sp"
                            android:paddingBottom="10sp"
                            android:layout_marginEnd="10sp"
                            android:layout_marginStart="10sp"
                            android:layout_weight="1"
                            android:gravity="start"
                            android:textStyle="bold"/>
                        <Button
                            android:id="@+id/gameNight_gameProposal_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="10sp"
                            android:layout_weight="2"
                            android:background="@drawable/buttton"
                            android:gravity="center"
                            android:text="Hinzufügen"
                            android:textColor="@color/white"
                            android:textSize="13sp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/gameNight_gameProposal_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                    </LinearLayout>
                    <Button
                        android:id="@+id/gameNight_GameVote_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="100sp"
                        android:layout_marginStart="100sp"
                        android:layout_marginTop="20sp"
                        android:layout_marginBottom="20sp"
                        android:background="@drawable/buttton"
                        android:gravity="center"
                        android:text="Abstimmen"
                        android:textColor="@color/white"
                        android:textSize="13sp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/gameNight_foodVoting_cardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="8dp"
                app:cardUseCompatPadding="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/gameNight_proposalHeader_food"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/tintedBlue"
                        android:text="Essensrichtung"
                        android:textSize="15sp"
                        android:paddingTop="10sp"
                        android:paddingBottom="10sp"
                        android:textColor="@color/lightGold"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:id="@+id/gameNight_foodProposal_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                    </LinearLayout>
                    <Button
                        android:id="@+id/gameNight_foodVote_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="100sp"
                        android:layout_marginStart="100sp"
                        android:layout_marginTop="20sp"
                        android:layout_marginBottom="20sp"
                        android:background="@drawable/buttton"
                        android:gravity="center"
                        android:text="Abstimmen"
                        android:textColor="@color/white"
                        android:textSize="13sp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:background="@color/blue"
                android:gravity="center"
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:text="Bestellung"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/header_layout" />
            <androidx.cardview.widget.CardView
                android:id="@+id/gameNight_foodOrders_deliveryService_cardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="8dp"
                app:cardUseCompatPadding="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/gameNight_foodOrder_header_deliveryService"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/tintedBlue"
                        android:text="Essenslieferant"
                        android:textSize="15sp"
                        android:paddingTop="10sp"
                        android:paddingBottom="10sp"
                        android:textColor="@color/lightGold"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/gameNight_deliveryService_name_value"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="15sp"
                        android:layout_gravity="center"
                        android:paddingStart="10sp"
                        android:paddingTop="10sp"
                        android:paddingBottom="10sp"
                        android:layout_marginTop="10sp"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/gameNight_deliveryServiceUrl_value"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="15sp"
                        android:autoLink="web"
                        android:layout_gravity="center"
                        android:paddingStart="10sp"
                        android:paddingBottom="10sp"
                        android:layout_marginTop="20sp"
                        android:layout_marginBottom="10sp"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:textStyle="bold"/>
                    <Button
                        android:id="@+id/gameNight_deliveryService_button"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="100sp"
                        android:layout_marginStart="100sp"
                        android:layout_marginTop="10sp"
                        android:layout_marginBottom="20sp"
                        android:layout_weight="1"
                        android:background="@drawable/buttton"
                        android:gravity="center"
                        android:text="Hinzufügen / Ändern"
                        android:textColor="@color/white"
                        android:textSize="13sp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/gameNight_foodOrders_orders_cardView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                app:cardElevation="8dp"
                app:cardUseCompatPadding="true">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/gameNight_foodOrder_header_orders"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/tintedBlue"
                        android:text="Bestellungen"
                        android:textSize="15sp"
                        android:paddingTop="10sp"
                        android:paddingBottom="10sp"
                        android:textColor="@color/lightGold"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:id="@+id/gameNight_foodOrder_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                    </LinearLayout>
                    <Button
                        android:id="@+id/gameNight_foodOrder_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="100sp"
                        android:layout_marginStart="100sp"
                        android:layout_marginTop="20sp"
                        android:layout_marginBottom="20sp"
                        android:background="@drawable/buttton"
                        android:gravity="center"
                        android:text="Hinzufügen/ Ändern"
                        android:textColor="@color/white"
                        android:textSize="13sp"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="@color/blue"
                android:paddingTop="10sp"
                android:layout_marginTop="20sp">
            <Button
                android:id="@+id/gameNight_cancel_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:layout_weight="1"
                android:background="@drawable/buttton"
                android:text="Zurück"
                android:textColor="@color/white"
                android:textSize="16sp"/>
            <Button
                android:id="@+id/gameNight_rate_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="10sp"
                android:layout_marginEnd="10sp"
                android:layout_marginBottom="10sp"
                android:layout_weight="1"
                android:background="@drawable/buttton"
                android:text="Abend bewerten"
                android:textColor="@color/white"
                android:textSize="16sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/blue"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/gameNight_message_button"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="10sp"
                    android:layout_marginEnd="10sp"
                    android:layout_marginBottom="10sp"
                    android:layout_weight="1"
                    android:background="@drawable/buttton"
                    android:text="Nachricht an Gruppe senden"
                    android:textColor="@color/black"
                    android:backgroundTint="@color/white"
                    android:textSize="16sp"/>
                <Button
                    android:id="@+id/gameNight_deleteGameNight_button"
                    android:layout_width="match_parent"
